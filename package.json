{
  "name": "webpack-config-cli",
  "version": "0.0.0",
  "description": "webpack configuration client ",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && nyc mocha src/**/*.test.js",
    "watch:test": "npm run test -w",
    "lint": "eslint . --fix",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nlarche/webpack-config-cli.git"
  },
  "keywords": [
    "webpack",
    "config",
    "client",
    "cli"
  ],
  "author": "Nicolas LARCHE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nlarche/webpack-config-cli/issues"
  },
  "homepage": "https://github.com/nlarche/webpack-config-cli#readme",
  "dependencies": {
    "commander": "^2.9.0",
    "inquirer": "^1.0.2",
    "npmlog": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "codecov": "^3.6.5",
    "eslint": "^2.9.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "mocha": "^2.4.5",
    "nyc": "^6.4.4"
  },
  "nyc": {
    "exclude": [
      "**/*.test.js",
      "test/**/*.js"
    ]
  }
}
